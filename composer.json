{
    "name": "phpeel/apricot-lib",
    "description": "The Phpeel PHP framework's utility common libraries.",
    "license": "MIT",
    "keywords": [
        "Phpeel lib",
        "Phpeel library",
        "Phpeel common library"
    ],
    "homepage": "https://github.com/phpeel/apricot-lib",
    "authors": [
        {
            "name" : "Apricot Library Contributors",
            "homepage": "https://github.com/phpeel/apricot-lib/contributors"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "phpeel/citron-di": "dev-develop",
        "symfony/yaml": "2.4.*"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "squizlabs/php_codesniffer": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "Phpeel\\ApricotLib\\": "src/",
            "Phpeel\\ApricotLib\\Tests\\": "tests/src/"
        }
    }
}
